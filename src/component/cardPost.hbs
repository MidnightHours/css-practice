{{#with data}}
<div class="post">
    <strong class="post_title">{{title}}</strong>
    <div class="post_date">{{date}} by <span class="post_author">{{author}}</span></div>
    <p class="post_description">{{description}}</p>
    <a class="preview_link" href="#">
        Continue reading
    </a>
</div>
{{/with}}